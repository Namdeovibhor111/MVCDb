@model IEnumerable<Emp>
<h2>Employee List</h2>
<hr />
<a asp-controller="Emp" asp-action="Create">New Employee</a>
<br />
<table border="1">
	<thead>
		<tr>
			<th>@Html.DisplayNameFor(e => e.Id)</th>
	    	<th>@Html.DisplayNameFor(e => e.Name)</th>
	    	<th>@Html.DisplayNameFor(e => e.Deptid)</th>
		    <th>@Html.DisplayNameFor(e => e.Dob)</th>
	   	</tr>
	</thead>
	<tbody>
			@foreach(var data in Model){
				<tr>
					<td>@Html.DisplayFor(e=> data.Id)</td>
					<td>@Html.DisplayFor(e=> data.Name)</td>
					<td>@Html.DisplayFor(e=> data.Dept.Name)</td>
					<td>@Html.DisplayFor(e=> data.Dob)</td>
					<td>
						<a asp-controller="Emp" asp-action="Edit" asp-route-id="@data.Id" >Edit</a>
						<a asp-controller="Emp" asp-action="Edit" asp-route-id="@data.Id" >Display</a>
					</td>
				</tr>
			}
	</tbody>
</table>